generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
}

model Form{
  form_id String @id @map("_id")
  user_id String 
  sch_id String 
  Apv_status RequestStatus
}

enum RequestStatus {
  PENDING_FACULTY
  PENDING_DEAN
  PRNDING_EDIT
  PENDING_JUDGING
  APPROVED
  REJECTED
}



